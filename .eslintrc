{
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:react/recommended",
      "jest-enzyme"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "browser": true,
      "mocha": true,
      "node": true,

      "jest": true
      
    },
    "rules": {
      "valid-jsdoc": 2,
      "react/jsx-uses-react": 1,
      "react/jsx-no-undef": 2,
      "react/jsx-wrap-multilines": 2,
      "react/no-string-refs": 0,
      "react/prop-types": [1, {"ignore": ["className", "children", "style"]}]
    },
    "plugins": [
      "import",
      "react"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }